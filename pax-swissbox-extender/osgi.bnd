Bundle-SymbolicName:\
  ${bundle.symbolicName}

Bundle-Version>:\
  ${pom.version}

Import-Package:\
  ${bundle.namespace},\
  org.apache.commons.logging; version="[1.0.4,2)",\
  org.ops4j.lang; version="${dependency.base.version}",\
  org.ops4j.pax.swissbox.lifecycle; version="[0.1,2)",\
  org.osgi.framework; version="[1.3,2)"

Export-Package:\
  ${bundle.namespace}; version="${pom.version}"

-versionpolicy:\
  [${version;==;${@}},${version;+;${@}})  
