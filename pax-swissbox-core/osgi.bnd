Bundle-SymbolicName:\
  ${bundle.symbolicName}

Bundle-Version>:\
  ${pom.version}

Import-Package:\
  ${bundle.namespace},\
  org.osgi.framework; version="[1.3,2)",\
  org.ops4j.lang; version="${dependency.base.version}"

Export-Package:\
  ${bundle.namespace}; version="${pom.version}"

-versionpolicy:\
  [${version;==;${@}},${version;+;${@}})