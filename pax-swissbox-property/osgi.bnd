Bundle-SymbolicName:\
  ${bundle.symbolicName}

Bundle-Version>:\
  ${pom.version}

Import-Package:\
  ${bundle.namespace},\
  org.ops4j.lang; version="${dependency.base.version}",\
  org.ops4j.util.property; version="${dependency.base.version}",\
  org.osgi.framework;version="[1.3,2)"

Export-Package:\
  ${bundle.namespace}; version="${pom.version}"

-versionpolicy:\
  [${version;==;${@}},${version;+;${@}})  
