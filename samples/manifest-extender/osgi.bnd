Bundle-Activator:\
  ${bundle.namespace}.internal.Activator

Bundle-Version>:\
  ${pom.version}

Bundle-SymbolicName:\
  ${bundle.symbolicName}

Import-Package:\
  org.slf4j; version="[1.4,2)",\
  org.osgi.framework; version="[1.0.0,2.0.0)",\
  *

Export-Package:\
  !*

Private-Package:\
  ${bundle.namespace}.internal.*  

-removeheaders:\
  Include-Resource,\
  Private-Package,\
  Embed-Dependency,\
  Embed-Transitive
  
